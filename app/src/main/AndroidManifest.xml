<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.app_phone_store_manager">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CAMERA"/>
    <application
        android:allowBackup="true"
        android:icon="@drawable/call"
        android:label="@string/app_name"
        android:roundIcon="@drawable/call"
        android:supportsRtl="true"
        android:theme="@style/Theme.App_phone_store_manager_Nhom_7">
        <activity
            android:name="com.example.app_phone_store_manager.LoginActivity"
            android:theme="@style/Theme.App_phone_store_manager_Nhom_7.NoActionBar" />
        <activity
            android:name="com.example.app_phone_store_manager.SplashScreenActivity"
            android:theme="@style/Theme.App_phone_store_manager_Nhom_7.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.app_phone_store_manager.MainActivity"
            android:label="@string/app_name"
            android:theme="@style/Theme.App_phone_store_manager_Nhom_7.NoActionBar" />
    </application>

</manifest>